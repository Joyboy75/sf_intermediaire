{% extends "admin/base.html.twig" %}



{% block main %}

{% for message in app.flashes('notice') %}
		<h3>{{ message }}</h3>
	{% endfor %}

	{% for category in categories %}


<div class="card">
		<h2>
			<a href="{{ path("admin_category_show", {'id' : category.id } ) }}">{{category.name}}</a>
		</h2>

</div>

	{% endfor %}

{% endblock %}
